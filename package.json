{
  "name": "ng-angularfire-testbed",
  "version": "0.0.0",
  "scripts": {
    "--- emulator ---": "",
    "emulator-demo-1": "firebase emulators:start --only firestore,auth --project demo-1",    
    "emulator-exec-test": "firebase emulators:exec --only firestore,auth --project demo-1 'npm test -- --forceExit'",
    "emulator-export": "firebase emulators:export emulator-data  --project demo-1",
    "emulator-import": "firebase emulators:start --only firestore,auth --project demo-1 --import emulator-data",      
    "--- start ---": "",
    "start": "ng serve",
    "start:emulator-demo": "ng serve --configuration=emulator-demo",
    "start:emulator-demo-prod": "ng serve --configuration=emulator-demo-prod",    
    "start:emulator-real": "ng serve --configuration=emulator-real",
    "start:production": "ng serve --configuration=production",
    "--- cypress ---": "",
    "cypress:open:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:open",
    "cypress:run:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:run",
    "cypress:emulate-real": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" GCLOUD_PROJECT=\"emulators-codelab-a5a89\" npm run cypress:open",
    "cypress:open": "cypress open",
    "cypress:open-real": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"serviceAccount-real.json\" npm run cypress:open",
    "cypress:run": "cypress run --e2e --browser chrome --config video=false",
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --runInBand",
    "test-all-emulator": "jest --runInBand --group=emulator-required",
    "test-all-no-emulator": "jest --group=-emulator-required",
    "lint": "ng lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.12",
    "@angular/common": "14.2.12",
    "@angular/compiler": "14.2.12",
    "@angular/core": "14.2.12",
    "@angular/fire": "7.6.1",
    "@angular/forms": "14.2.12",
    "@angular/platform-browser": "14.2.12",
    "@angular/platform-browser-dynamic": "14.2.12",
    "@angular/router": "14.2.12",
    "firebase": "9.23.0",
    "rxfire": "6.0.5",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.13",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "14.2.13",
    "@angular/compiler-cli": "14.2.12",
    "@firebase/rules-unit-testing": "2.0.7",
    "@types/jest": "28.1.8",
    "@types/node": "16.18.126",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "cross-env": "7.0.3",
    "cypress": "10.11.0",
    "cypress-firebase": "2.2.5",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.2",
    "eslint-plugin-chai-friendly": "0.7.4",
    "eslint-plugin-cypress": "2.15.2",
    "firebase-admin": "11.11.1",
    "firebase-tools": "11.30.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-preset-angular": "12.2.6",
    "jest-runner-groups": "2.2.0",
    "prettier": "2.8.8",
    "ts-snippet": "5.0.2",
    "typescript": "4.7.4"
  }
}
